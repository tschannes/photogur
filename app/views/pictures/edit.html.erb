<h1>Edit Picture</h1>

<nav>
  <%= link_to "Back", @picture %>
</nav>

<%= form_for @picture do |f| %>
<%= render 'shared/error_messages', object: f.object %>
  <p>
    <%= f.label "artist" %><br>
    <%= f.text_field "artist" %>
  </p>
  <p>
    <%= f.label "title" %><br>
    <%= f.text_field "title" %>
  </p>
  <p>
    <%= f.label "url" %><br>
    <%= f.text_field "url" %>
  </p>
  <p>
    <%= submit_tag "Save", id: "submit" %>
  </p>
<% end %>