<h1>Add a picture</h1>
<div class="pure-group">
  <%= form_for @picture do |f| %>
    <p>
      <%= f.label "artist" %><br>
      <%= f.text_field "artist" %>
    </p>
    <p>
      <%= f.label "title" %><br>
      <%= f.text_field "title" %>
    </p>
    <p>
      <%= f.label "url" %><br>
      <%= f.text_field "url" %>
    </p>
    <p id="submit">
      <%= f.submit "Save", id: "submit" %> 
    </p>
  <% end %>
</div>